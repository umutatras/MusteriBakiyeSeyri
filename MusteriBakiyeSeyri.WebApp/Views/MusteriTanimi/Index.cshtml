@model List<MusteriBakiyeSeyri.DTOs.MusteriTanim.MusteriTanimListDto>
@{

}

<div class="container mt-4">
    <h3>Müşteri Listesi</h3>
    <table class="table table-striped table-bordered">
        <thead class="table-dark">
            <tr>
                <th>ID</th>
                <th>Unvan</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {

            <tr>
                <td>@item.Id</td>
                <td>@item.Unvan</td>
            </tr>
            }
         
        </tbody>
    </table>
</div>
